{
  "mappings": [
    { "match": ["flex"], "sx": { "display": "flex" } },
    { "match": ["items-center"], "sx": { "alignItems": "center" } },
    { "match": ["justify-center"], "sx": { "justifyContent": "center" } },
    { "match": ["gap-1"], "sx": { "gap": "0.25rem" } },
    { "match": ["gap-2"], "sx": { "gap": "0.5rem" } },
    { "match": ["gap-3"], "sx": { "gap": "0.75rem" } },
    { "match": ["rounded-md"], "sx": { "borderRadius": "0.375rem" } },
    { "match": ["px-4"], "sx": { "paddingLeft": "1rem", "paddingRight": "1rem" } },
    { "match": ["py-2"], "sx": { "paddingTop": "0.5rem", "paddingBottom": "0.5rem" } }
  ]
}
